  String _getFieldTypeLabel(String type) {
    switch (type) {
      case 'text':
        return 'Metin';
      case 'longtext':
        return 'Uzun Metin';
      case 'number':
        return 'SayÄ±';
      case 'date':
        return 'Tarih';
      case 'datetime':
        return 'Tarih & Saat';
      case 'dropdown':
        return 'SeÃ§im';
      case 'checkbox':
        return 'Evet/HayÄ±r';
      case 'file':
        return 'Dosya Ekleme';
      default:
        return 'Metin';
    }
  }

